<%- |  String $operationType,
       String $globaldb_name,
       String $db_name,
       Optional[String] $cluster_nodes = undef,
       String $sys_password,
       String $system_password,
       String $em_configuration,
       String $db_snmp_password,
       Optional[String] $data_file_destination = undef,
       Optional[String] $recovery_area_destination = undef,
       Optional[String] $storage_type = undef,
       Optional[String] $asm_diskgroup = undef,
       Optional[String] $asm_snmp_password = undef,
       Optional[String] $recovery_diskgroup = undef,
       String $character_set,
       Optional[String] $nationalcharacter_set = undef,
       Optional[String] $sanitizedInitParams = undef,
       Optional[String] $sample_schema = undef,
       Integer $memory_percentage,
       String $database_type,
       Integer $memory_total,
       Optional[Integer] $db_port,
       Boolean $container_database,
       Boolean $automatic_memory_management
 | -%>

